class Solution {
public:
    vector<int> nextGreaterElement(vector<int>& nums1, vector<int>& nums2) {
          vector<int> v;
        map<int,int>m;
        for (int i = 0; i < nums2.size(); i++) m[nums2[i]]=i+1;
        for (int i = 0; i < nums1.size(); i++) {
            int num = -1;
            if(m[nums1[i]])
        for (int j = m[nums1[i]]; j < nums2.size(); j++) {
            if (nums2[j] > nums1[i]) {
                num = nums2[j];
                break;
                               
            }
        }
            v.push_back(num);
        }
        return v;
    }
};
